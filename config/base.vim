vim9script

# Vim Base Settings. 
# set no compatible with vi.
set nocompatible

# line number setting.
set number
set relativenumber

# cursor settings.
set cursorline
# set cursorcolumn
# set guicursor


# syntax on.
syntax on

# search settings.
set hlsearch
set incsearch

# add tab space
set ts=4
set softtabstop=4
set shiftwidth=4
set expandtab
set autoindent
